msgid "plugins.generic.browseBySection.name"
msgstr "Nach Rubriken durchblättern"

msgid "plugins.generic.browseBySection.description"
msgstr "Erlaubt Besucher/innen, die Zeitschrift nach Rubriken durchzublättern."

msgid "plugins.generic.browseBySection.emptySection"
msgstr "In dieser Rubrik wurde bislang kein Beitrag veröffentlicht."

msgid "plugins.generic.browseBySection.browsingLabel"
msgstr "Nach Rubriken durchblättern"

msgid "plugins.generic.browseBySection.enableBrowsing"
msgstr "Ermöglicht den Zugriff auf das Archiv mit allen veröffentlichten Artikeln einer Rubrik."

msgid "plugins.generic.browseBySection.browseByPathLabel"
msgstr "URL-Pfad der Rubrik"

msgid "plugins.generic.browseBySection.browseByPathDescription"
msgstr "Passe den URL-Pfad an, über den Besucher/innen auf das Archiv der Rubrik zugreifen können. Der Pfad darf nur Buchstaben, Zahlen und Bindestriche enthalten."

msgid "plugins.generic.browseBySection.browseByPerPageLabel"
msgstr "Beiträge pro Seite"

msgid "plugins.generic.browseBySection.browseByPerPageDescription"
msgstr "Zahl der Beiträge, die auf einer Seite des Rubrik-Archivs dargestellt werden."

msgid "plugins.generic.browseBySection.browseByDescriptionLabel"
msgstr "Beschreibung des Rubrik-Archivs"

msgid "plugins.generic.browseBySection.navMenuItem"
msgstr "Rubrik: {$name}"

msgid "plugins.generic.browseBySection.navMenuItem.description"
msgstr "Link zu dem Archiv der veröffentlichten Beiträge in dieser Rubrik."

msgid "plugins.generic.browseBySection.orderBy"
msgstr "sortieren nach"
